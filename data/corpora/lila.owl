    <!-- http://lila-erc.eu/ontologies/lila/annotation -->

    <owl:ObjectProperty rdf:about="http://lila-erc.eu/ontologies/lila/annotation">
        <rdfs:comment>General property for morphological annotation (including POS)</rdfs:comment>
        <rdfs:label>morphological annotation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://lila-erc.eu/ontologies/lila/hasAffix -->

    <owl:ObjectProperty rdf:about="http://lila-erc.eu/ontologies/lila/hasAffix">
        <rdfs:subPropertyOf rdf:resource="http://lila-erc.eu/ontologies/lila/wordFormation"/>
        <rdfs:label>has affix</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://lila-erc.eu/ontologies/lila/hasBase -->

    <owl:ObjectProperty rdf:about="http://lila-erc.eu/ontologies/lila/hasBase">
        <rdfs:subPropertyOf rdf:resource="http://lila-erc.eu/ontologies/lila/wordFormation"/>
        <owl:inverseOf rdf:resource="http://lila-erc.eu/ontologies/lila/isBase"/>
        <rdfs:range rdf:resource="http://lila-erc.eu/ontologies/lila/Base"/>
        <rdfs:label>has base</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://lila-erc.eu/ontologies/lila/hasDegree -->

    <owl:ObjectProperty rdf:about="http://lila-erc.eu/ontologies/lila/hasDegree">
        <rdfs:subPropertyOf rdf:resource="http://lila-erc.eu/ontologies/lila/annotation"/>
        <owl:inverseOf rdf:resource="http://lila-erc.eu/ontologies/lila/isDegree"/>
        <rdfs:range rdf:resource="http://lila-erc.eu/ontologies/lila/Degree"/>
        <rdfs:label>has degree</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://lila-erc.eu/ontologies/lila/hasGender -->

    <owl:ObjectProperty rdf:about="http://lila-erc.eu/ontologies/lila/hasGender">
        <rdfs:subPropertyOf rdf:resource="http://lila-erc.eu/ontologies/lila/annotation"/>
        <owl:inverseOf rdf:resource="http://lila-erc.eu/ontologies/lila/isGender"/>
        <rdfs:range rdf:resource="http://lila-erc.eu/ontologies/lila/Gender"/>
        <rdfs:label>has gender</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://lila-erc.eu/ontologies/lila/hasHypolemma -->

    <owl:ObjectProperty rdf:about="http://lila-erc.eu/ontologies/lila/hasHypolemma">
        <owl:inverseOf rdf:resource="http://lila-erc.eu/ontologies/lila/isHypolemma"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://lila-erc.eu/ontologies/lila/Lemma"/>
        <rdfs:range rdf:resource="http://lila-erc.eu/ontologies/lila/Hypolemma"/>
        <rdfs:label>has hypolemma</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://lila-erc.eu/ontologies/lila/hasInflectionType -->

    <owl:ObjectProperty rdf:about="http://lila-erc.eu/ontologies/lila/hasInflectionType">
        <owl:inverseOf rdf:resource="http://lila-erc.eu/ontologies/lila/isInflectionType"/>
        <rdfs:range rdf:resource="http://lila-erc.eu/ontologies/lila/InflectionType"/>
        <rdfs:label>has inflection type</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://lila-erc.eu/ontologies/lila/hasLemma -->

    <owl:ObjectProperty rdf:about="http://lila-erc.eu/ontologies/lila/hasLemma">
        <rdfs:range rdf:resource="http://lila-erc.eu/ontologies/lila/Lemma"/>
        <rdfs:comment>Property that links corpus tokens (NIF words) to lemmas of LiLa</rdfs:comment>
        <rdfs:label>has lemma</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://lila-erc.eu/ontologies/lila/hasPOS -->

    <owl:ObjectProperty rdf:about="http://lila-erc.eu/ontologies/lila/hasPOS">
        <rdfs:subPropertyOf rdf:resource="http://lila-erc.eu/ontologies/lila/annotation"/>
        <owl:inverseOf rdf:resource="http://lila-erc.eu/ontologies/lila/isPOS"/>
        <rdfs:range rdf:resource="http://lila-erc.eu/ontologies/lila/POS"/>
