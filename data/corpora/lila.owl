     -->

    


    <!-- http://lila-erc.eu/ontologies/lila/prosodicRepresentation -->

    <owl:DatatypeProperty rdf:about="http://lila-erc.eu/ontologies/lila/prosodicRepresentation">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/lemon/ontolex#representation"/>
        <rdfs:comment xml:lang="en">String with vowel length marked (brevis or longa). In most cases, not all vowels receive length mark, but only the ambiguous ones. E.g. &quot;occīdo&quot; (long &quot;i&quot;) vs &quot;occĭdo&quot; (short &quot;i&quot;)</rdfs:comment>
        <rdfs:label>prosodic representation</rdfs:label>
    </owl:DatatypeProperty>
    


