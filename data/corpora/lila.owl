    <owl:ObjectProperty rdf:about="http://lila-erc.eu/ontologies/lila/isDegree">
        <rdfs:subPropertyOf rdf:resource="http://lila-erc.eu/ontologies/lila/annotation"/>
        <rdfs:label>is degree</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://lila-erc.eu/ontologies/lila/isGender -->

    <owl:ObjectProperty rdf:about="http://lila-erc.eu/ontologies/lila/isGender">
        <rdfs:subPropertyOf rdf:resource="http://lila-erc.eu/ontologies/lila/annotation"/>
        <rdfs:label>is gender</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://lila-erc.eu/ontologies/lila/isHypolemma -->

    <owl:ObjectProperty rdf:about="http://lila-erc.eu/ontologies/lila/isHypolemma">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://lila-erc.eu/ontologies/lila/Hypolemma"/>
        <rdfs:range rdf:resource="http://lila-erc.eu/ontologies/lila/Lemma"/>
        <rdfs:label>is hypolemma</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://lila-erc.eu/ontologies/lila/isInflectionType -->

    <owl:ObjectProperty rdf:about="http://lila-erc.eu/ontologies/lila/isInflectionType">
        <rdfs:label>is inflection type</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://lila-erc.eu/ontologies/lila/isPOS -->

    <owl:ObjectProperty rdf:about="http://lila-erc.eu/ontologies/lila/isPOS">
        <rdfs:subPropertyOf rdf:resource="http://lila-erc.eu/ontologies/lila/annotation"/>
