        <owl:inverseOf rdf:resource="http://lila-erc.eu/ontologies/lila/isPrefix"/>
        <rdfs:range rdf:resource="http://lila-erc.eu/ontologies/lila/Prefix"/>
        <rdfs:label>has prefix</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://lila-erc.eu/ontologies/lila/hasSuffix -->

    <owl:ObjectProperty rdf:about="http://lila-erc.eu/ontologies/lila/hasSuffix">
        <rdfs:subPropertyOf rdf:resource="http://lila-erc.eu/ontologies/lila/hasAffix"/>
        <owl:inverseOf rdf:resource="http://lila-erc.eu/ontologies/lila/isSuffix"/>
        <rdfs:range rdf:resource="http://lila-erc.eu/ontologies/lila/Suffix"/>
        <rdfs:label>has suffix</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://lila-erc.eu/ontologies/lila/isBase -->

    <owl:ObjectProperty rdf:about="http://lila-erc.eu/ontologies/lila/isBase">
        <rdfs:subPropertyOf rdf:resource="http://lila-erc.eu/ontologies/lila/wordFormation"/>
        <rdfs:label>is base</rdfs:label>
    </owl:ObjectProperty>
    


