    <!-- http://lila-erc.eu/ontologies/lila/AdjectivalInflection -->

    <owl:Class rdf:about="http://lila-erc.eu/ontologies/lila/AdjectivalInflection">
        <rdfs:subClassOf rdf:resource="http://lila-erc.eu/ontologies/lila/InflectionType"/>
        <rdfs:label>Adjectival Inflection</rdfs:label>
    </owl:Class>
    


    <!-- http://lila-erc.eu/ontologies/lila/AdjectivalLemma -->

    <owl:Class rdf:about="http://lila-erc.eu/ontologies/lila/AdjectivalLemma">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://lila-erc.eu/ontologies/lila/hasPOS"/>
                <owl:allValuesFrom rdf:resource="http://lila-erc.eu/ontologies/lila/Adjective"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://lila-erc.eu/ontologies/lila/_by_inflection_type"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://lila-erc.eu/ontologies/lila/hasInflectionType"/>
                <owl:allValuesFrom rdf:resource="http://lila-erc.eu/ontologies/lila/AdjectivalInflection"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://lila-erc.eu/ontologies/lila/Adjective -->

    <owl:Class rdf:about="http://lila-erc.eu/ontologies/lila/Adjective">
        <rdfs:subClassOf rdf:resource="http://lila-erc.eu/ontologies/lila/POS"/>
        <rdfs:label>Adjective</rdfs:label>
    </owl:Class>
    


    <!-- http://lila-erc.eu/ontologies/lila/Adposition -->

    <owl:Class rdf:about="http://lila-erc.eu/ontologies/lila/Adposition">
        <rdfs:subClassOf rdf:resource="http://lila-erc.eu/ontologies/lila/POS"/>
        <rdfs:label>Adposition</rdfs:label>
    </owl:Class>
    


    <!-- http://lila-erc.eu/ontologies/lila/Adverb -->

    <owl:Class rdf:about="http://lila-erc.eu/ontologies/lila/Adverb">
        <rdfs:subClassOf rdf:resource="http://lila-erc.eu/ontologies/lila/POS"/>
        <rdfs:label>Adverb</rdfs:label>
    </owl:Class>
    


    <!-- http://lila-erc.eu/ontologies/lila/Affix -->

    <owl:Class rdf:about="http://lila-erc.eu/ontologies/lila/Affix">
        <rdfs:subClassOf rdf:resource="http://lila-erc.eu/ontologies/lila/Morpheme"/>
        <owl:disjointWith rdf:resource="http://lila-erc.eu/ontologies/lila/Base"/>
        <rdfs:label>Affix</rdfs:label>
    </owl:Class>
    


    <!-- http://lila-erc.eu/ontologies/lila/Auxiliary -->

    <owl:Class rdf:about="http://lila-erc.eu/ontologies/lila/Auxiliary">
        <rdfs:subClassOf rdf:resource="http://lila-erc.eu/ontologies/lila/Verb"/>
        <rdfs:label>Auxiliary verb</rdfs:label>
    </owl:Class>
    


    <!-- http://lila-erc.eu/ontologies/lila/Base -->

    <owl:Class rdf:about="http://lila-erc.eu/ontologies/lila/Base">
        <rdfs:subClassOf rdf:resource="http://lila-erc.eu/ontologies/lila/Morpheme"/>
        <rdfs:comment xml:lang="en">A (Lexical) Base is a morpheme which defines the common lexical element of a word that is shared by forms belonging to the same derivational family. E.g.: socialis, socius, sociare etc.

Note that in LiLa we don&apos;t describe the Base, we don&apos;t even attribute a Written Representation or a descriptive label</rdfs:comment>
        <rdfs:label>Base</rdfs:label>
    </owl:Class>
    


    <!-- http://lila-erc.eu/ontologies/lila/CommonNoun -->

    <owl:Class rdf:about="http://lila-erc.eu/ontologies/lila/CommonNoun">
        <rdfs:subClassOf rdf:resource="http://lila-erc.eu/ontologies/lila/Noun"/>
        <rdfs:label>Common Noun</rdfs:label>
    </owl:Class>
    


